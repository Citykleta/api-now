{
  "name": "citykleta-api-now",
  "version": "1.0.0",
  "description": "citykleta api ",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "node ./test/index.js | tap-mocha-reporter classic",
    "test:coverage": "c8 -r html npm test",
    "build": "tsc",
    "build:watch": "npm run build -- -w",
    "serve:watch": "nodemon ./scripts/dev-server.js",
    "dev": "npm-run-all -p *:watch",
    "build:media": "node ./scripts/get_municipalities_as_GeoJson.js | tee ./media/havana.json | geojson-to-svg > ./media/havana.svg",
    "now-build": "NODE_ENV=production npm run build"
  },
  "keywords": [
    "citykleta",
    "api",
    "rest",
    "bicycle",
    "cuba"
  ],
  "author": "Laurent RENARD",
  "license": "MIT",
  "devDependencies": {
    "@types/koa": "^2.0.49",
    "c8": "^5.0.1",
    "dotenv": "^8.0.0",
    "find-my-way": "^2.1.0",
    "geojson-to-svg-cli": "^1.0.0",
    "globby": "^10.0.1",
    "nock": "^10.0.6",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "supertest": "^4.0.2",
    "tap-mocha-reporter": "^4.0.1",
    "typescript": "^3.5.3",
    "zora": "^3.0.1"
  },
  "dependencies": {
    "@citykleta/habana-address-normalizer": "^0.1.1",
    "@koa/cors": "^3.0.0",
    "@mapbox/mapbox-sdk": "^0.6.0",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-json-schema": "^3.0.0",
    "koa-logger": "^3.2.0",
    "pg": "^7.11.0"
  }
}
